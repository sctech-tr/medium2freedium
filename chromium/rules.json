{
    "rules": [
      {
        "id": "redirectMediumToFreedium",
        "condition": {
          "urlFilter": ".*medium\\.com.*",
          "resourceTypes": ["main_frame"]
        },
        "action": {
          "type": "redirect",
          "redirect": {
            "regexSubstitution": "https://freedium.cfd/$0"
          }
        }
      }
    ]
  }
  