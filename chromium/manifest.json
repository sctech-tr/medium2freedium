{
  "manifest_version": 3,
  "name": "Medium to Freedium",
  "version": "1.0",
  "description": "Prepends https://freedium.cfd/ to Medium URLs.",
  "permissions": [
    "declarativeNetRequest",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "*://*.medium.com/*"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  }
}
